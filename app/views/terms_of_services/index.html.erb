<p id="notice"><%= notice %></p>

<h1>Listing Terms Of Services</h1>

<table>
  <thead>
    <tr>
      <th>Store name</th>
      <th>Terms</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @terms_of_services.each do |terms_of_service| %>
      <tr>
        <td><%= terms_of_service.store_name %></td>
        <td><%= terms_of_service.terms %></td>
        <td><%= link_to 'Show', terms_of_service %></td>
        <td><%= link_to 'Edit', edit_terms_of_service_path(terms_of_service) %></td>
        <td><%= link_to 'Destroy', terms_of_service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Terms of service', new_terms_of_service_path %>
